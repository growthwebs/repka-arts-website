            },
            "accordion_row_jLb8PE": {
              "type": "_accordion-row",
              "name": "t:names.accordion_dash_row",
              "settings": {
                "heading": "How do I book my spot?",
                "open_by_default": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_X6tGjC": {
                  "type": "text",
                  "name": "t:names.text",
                  "settings": {
                    "text": "<p>It's simple! Just add the event you want to your cart and complete the checkout. Spaces are limited, so this ticket secures your spot. We'll send you a confirmation email with all the details within 24 hours.</p>",
                    "width": "100%",
                    "max_width": "none",
                    "alignment": "left",
                    "type_preset": "rte",
