              ]
            },
            "accordion_row_EqPbef": {
              "type": "_accordion-row",
              "settings": {
                "heading": "Do I need to bring anything with me?",
                "open_by_default": false,
                "icon": "none",
                "width": 20
              },
              "blocks": {
                "text_wnpcDT": {
                  "type": "text",
                  "settings": {
                    "text": "<p>Nope, just yourself. We provide everything: your canvas, paints, brushes, and an apron.</p>",
                    "width": "100%",
                    "max_width": "none",
                    "alignment": "left",
                    "type_preset": "rte",
