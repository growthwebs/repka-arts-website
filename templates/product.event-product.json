              "blocks": {
                "text_WgTCdj": {
                  "type": "text",
                  "name": "t:names.text",
                  "settings": {
                    "text": "<p>Yes! We always have snacks and a drink for you. Depending on the event, this could be coffee and brownies, or wine and cheese, depending on the venue. Itâ€™s all part of the fun.</p>",
                    "width": "100%",
                    "max_width": "none",
