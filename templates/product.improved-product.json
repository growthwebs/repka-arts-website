              "blocks": {
                "text_gH7qfA": {
                  "type": "text",
                  "name": "t:names.text",
                  "settings": {
                    "text": "<p>Payments at Repka Arts are 100% safe and secure. We accept debit or credit cards (Visa, Maestro, Mastercard & American Express), PayPal, Shop Pay, Google Pay, Apple pay. All our transactions are secure.</p>",
                    "width": "100%",
